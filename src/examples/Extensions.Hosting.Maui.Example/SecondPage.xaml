<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="Extensions.Hosting.Maui.Example.SecondPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Second Page">
    <VerticalStackLayout>
        <Label
            HorizontalOptions="Center"
            Text="Welcome to the second page!"
            VerticalOptions="Center" />
        <Button
            x:Name="NavigateBtn"
            Clicked="OnNavigateClicked"
            HorizontalOptions="Center"
            SemanticProperties.Hint="Navigates to the main page"
            Text="Go back" />
    </VerticalStackLayout>
</ContentPage>
